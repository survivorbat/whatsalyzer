{
  "root": true,
  "env": {
    "browser": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": ["react-app", "airbnb", "airbnb-typescript"],
  "rules": {
    "no-param-reassign": "off",
    "max-len": "off"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}
